{
    "name": "eonx-com/easy-event-dispatcher",
    "description": "Framework agnostic event dispatcher",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEventDispatcher\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
